{
  "name": "@zen/tui-router",
  "version": "0.0.0",
  "description": "Router components for @zen/tui - client-side routing for terminal UIs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bunup src/index.ts --format esm --dts --target node --splitting=false --external @zen/router-core --external @zen/tui --external @zen/runtime --external @zen/signal",
    "build:watch": "bunup src/index.ts --format esm --dts --target node --splitting=false --watch --external @zen/router-core --external @zen/tui --external @zen/runtime --external @zen/signal",
    "dev": "bun run --watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["zen", "tui", "router", "terminal", "cli", "routing"],
  "author": "Zen Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-tui-router"
  },
  "dependencies": {
    "@zen/router-core": "workspace:*"
  },
  "peerDependencies": {
    "@zen/tui": "workspace:*",
    "@zen/runtime": "workspace:*",
    "@zen/signal": "workspace:*"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
