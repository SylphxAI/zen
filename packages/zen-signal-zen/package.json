{
  "name": "@zen/signal-zen",
  "version": "0.0.0",
  "description": "Zen framework adapter for Zen Signal reactive primitives",
  "author": "Sylphx <hi@sylphx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/zen.git",
    "directory": "packages/zen-signal-zen"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "keywords": ["zen", "signal", "reactive", "lifecycle"],
  "homepage": "https://github.com/SylphxAI/zen/tree/main/packages/zen-signal-zen#readme",
  "bugs": {
    "url": "https://github.com/SylphxAI/zen/issues"
  },
  "scripts": {
    "build": "bunup --format esm,cjs --dts --target node --minify",
    "clean": "bun rimraf dist .turbo"
  },
  "dependencies": {
    "@zen/signal": "workspace:*"
  },
  "peerDependencies": {
    "@zen/zen": "workspace:*"
  },
  "devDependencies": {
    "bunup": "^0.15.13",
    "@types/node": "^22.14.1",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
